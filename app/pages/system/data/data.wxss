/* pages/system/data/data.wxss */
@import "./template/item.wxss";

page{ background-color: #EEEEEE;}

.post{
	width: 100%;
	height: 100rpx;
	/* border: 1rpx solid #555555; */
	background-color: #fff;
	display: flex;
	justify-content: center;
	align-items: center;
	position: fixed;
	bottom: 10rpx;
	border-radius: 25rpx;
}
.post view{
	margin-left: 50rpx;
}
.post button{
	width: 200rpx;
	height: 80rpx;
	font-size: 30rpx;
	font-weight: bold;
	color: #FFFFFF;
	background: #FFA500;
	border-radius: 50rpx;
}

.add{
  margin: 20rpx 50rpx;
  height: 160rpx;
	border: 5rpx dashed #13BEEC;
	border-radius: 15px;
	display: flex;
	justify-content: center;
  flex-direction: center;
}

.add {
  font-size: 100rpx;
  color: #13BEEC;
}

#list {
	margin-top: 10rpx;
}

.sp {
	color: #13BEEC;
	display: flex;
	justify-content:center;
}

.void{
	height: 120rpx;
}